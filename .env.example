# Variáveis de ambiente para o projeto Agno Playground

# Google Gemini API Key (obrigatório)
GOOGLE_API_KEY=your_google_api_key_here

# OpenAI API Key (opcional)
OPENAI_API_KEY=your_openai_api_key_here

# Google API Key (opcional)
GOOGLE_API_KEY=your_google_api_key_here

# Configurações do servidor
HOST=localhost
PORT=7777

# Configurações de storage
STORAGE_DIR=storage
DB_FILE=agents.db

# Configurações de MCP
MCP_ENABLED=true
MCP_SERVER_HOST=localhost
MCP_SERVER_PORT=8888

# GitHub API Token (opcional - para MCP GitHub)
# Obtenha em: https://github.com/settings/tokens
GITHUB_TOKEN=your_github_token_here

# Outros MCPs (adicione conforme necessário)
# SLACK_TOKEN=your_slack_token_here
# DISCORD_TOKEN=your_discord_token_here

# Configurações de logging
LOG_LEVEL=INFO
LOG_FILE=logs/agno_playground.log
